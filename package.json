{
    "name": "ttz",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www"
    },
    
    "browserify": {
        "transform": [
            "browserify-shim"
        ]
    },
    "main": "./bin/www",
    "browser": {
        "jquery": "./bower_components/jquery/dist/jquery.js",
        "fullpage":"./bower_components/fullpage/jquery.fullPage.js"
    },
    "browserify-shim": {
        "jquery": "$",
        "fullpage": {
            "exports": "fullpage",
            "depends": [
                "jquery:jQuery"
            ]
        }
    },

    "dependencies": {
        "body-parser": "~1.12.0",
        "cookie-parser": "~1.3.4",
        "debug": "~2.1.1",
        "ejs": "~2.3.1",
        "express": "~4.12.2",
        "moment": "~2.10.2",
        "morgan": "~1.5.1",
        "serve-favicon": "~2.2.0"
    },
    "devDependencies": {
        "browserify": "^10.2.0",
        "browserify-shim": "^3.8.6",
        "gulp": "^3.8.11",
        "gulp-compass": "^2.0.4",
        "gulp-imagemin": "^2.2.1",
        "gulp-jshint": "^1.10.0",
        "imagemin-jpegtran": "^4.1.0",
        "imagemin-pngquant": "^4.1.0",
        "vinyl-source-stream": "^1.1.0"
    }
}
